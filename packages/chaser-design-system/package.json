{
  "name": "@jordan-mace/chaser-design-system",
  "version": "1.2.5",
  "type": "module",
  "sideEffects": [
    "*.css.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordan-mace/chaser-design-system.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./layers.css": {
      "types": "./dist/styles/layers.css.d.ts",
      "import": "./dist/styles/layers.css.js"
    },
    "./theme.css": {
      "types": "./dist/styles/theme.css.d.ts",
      "import": "./dist/styles/theme.css.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json --noCheck --skipLibCheck || true",
    "dev": "tsc --project tsconfig.build.json --watch --noCheck --skipLibCheck || true",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest --config=vitest.browser.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rslib/core": "^0.18.0",
    "@rstest/core": "^0.6.5",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/builder-webpack5": "^10.0.8",
    "@storybook/react-webpack5": "^10.0.8",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vanilla-extract/esbuild-plugin": "^2.3.21",
    "@vanilla-extract/integration": "^8.0.7",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.10",
    "css-loader": "^7.1.2",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.0",
    "eslint-plugin-storybook": "^10.0.8",
    "globals": "^16.4.0",
    "mini-css-extract-plugin": "^2.9.4",
    "playwright": "^1.58.0",
    "prettier": "^3.6.2",
    "storybook": "^10.1.8",
    "storybook-addon-rslib": "^3.1.0",
    "storybook-react-rsbuild": "^3.1.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.10",
    "vitest-browser-react": "^2.0.2"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/sprinkles": "^1.6.5",
    "clsx": "^2.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "@rsbuild/core": "^1.6.14"
  }
}
